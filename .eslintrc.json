{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "comma-dangle": "off",
        "no-console": "off",
        "eol-last": "off",
        "no-underscore-dangle": "off"
    }
}
